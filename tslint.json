{
  "extends": ["tslint-react"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "no-implicit-dependencies": false,
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "jsx-wrap-multiline": false
  },
  "rulesDirectory": []
}
